          <h3 class="callout-title"><%= t(:homeAdvantagesSixTitle) %></h3>
          <p class="callout-desc"><%= t(:homeAdvantagesSixDesc) %></p>
        </li>
      </ul>
    </div>
  </section>

  <section id="features" class="home-features">
    <div class="grid-container">
      <h2 class="home-features-title"><%= t(:homeFeatureTitle) %></h2>
      <p class="home-features-desc"><%= t(:homeFeatureDesc) %></p>
      <div class="feature-container">
        <article class="feature-item">
            <%= image_tag "feature_beratung.svg", :alt => t(:homeFeatureConsultingAlt), :class => 'feature-img' %>
            <h3 class="feature-title"><%= t(:homeFeatureConsultingTitle) %></h3>
            <p class="feature-desc"><%= t(:homeFeatureConsultingDesc) %></p>
        </article>
        <article class="feature-item">
          <%= image_tag "feature_community.svg", :alt =>  t(:homeFeatureCommunityAlt), :class => 'feature-img' %>
          <h3 class="feature-title"><%= t(:homeFeatureCommunityTitle) %></h3>
            <p class="feature-desc"><%= t(:homeFeatureCommunityDesc) %></p>
        </article>
        <article class="feature-item">
          <%= image_tag "feature_addons.svg", :alt => t(:homeFeatureAddonsAlt), :class => 'feature-img' %>
          <h3 class="feature-title"><%= t(:homeFeatureAddonsTitle) %></h3>
          <p class="feature-desc"><%= t(:homeFeatureAddonsDesc) %></p>
        </article>
      </div>
    </div>
  </section>

  <section id="offer" class="home-offer">
    <div class="grid-container">
